public class ClaimService : IClaimService
{
    // Existing code...

    public async Task<List<ClaimDto>> GetNewClaimsAsync()
    {
        var response = await _httpClient.GetAsync("Claim/new");
        response.EnsureSuccessStatusCode();

        var jsonResponse = await response.Content.ReadAsStringAsync();
        var newClaims = JsonSerializer.Deserialize<List<ClaimDto>>(jsonResponse, new JsonSerializerOptions { PropertyNameCaseInsensitive = true });

        return newClaims;
    }
}
