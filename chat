public async Task<List<PolicyHolderDto>> GetBlacklistedPolicyHoldersAsync()
{
    string endpoint = "PolicyHolder/blacklisted";
    var response = await _httpClient.GetAsync(endpoint);
    response.EnsureSuccessStatusCode();

    var jsonResponse = await response.Content.ReadAsStringAsync();
    var blacklistedCustomers = JsonSerializer.Deserialize<List<PolicyHolderDto>>(jsonResponse, new JsonSerializerOptions { PropertyNameCaseInsensitive = true });

    return blacklistedCustomers;
}
